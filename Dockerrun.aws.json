{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "landingpage",
      "image": "alexlee27/fm8landingpage",
      "hostName": "client",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3005
        }]
    },
    {
      "name": "prometheus",
      "image": "alexlee27/test-prometheus",
      "hostName": "prometheus",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 9090,
          "containerPort": 9090
        }]
    }
  ]
}